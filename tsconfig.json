{
  "compilerOptions": {
    "outDir": "discord-bot/dist",
    "rootDir": "discord-bot/src",
    "module": "commonjs",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": ["node"]
  },
  "include": ["discord-bot/src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
